{
    "manifest_version": 3,
    "name": "Founder Mode",
    "version": "1.0",
    "description": "Replace LinkedIn feed with a Founder Mode message to keep you focused.",
    "permissions": ["activeTab"],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.linkedin.com/feed/"
        ],
        "js": ["content.js"]
      }
    ]
  }
  